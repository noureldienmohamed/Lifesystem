<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nour.lifesystem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ball"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ball"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".controller.league" />
        <activity android:name=".controller.BaseActivity" />
        <activity android:name=".controller.skillActivity" />
        <activity android:name=".controller.FinishActivity"></activity>
    </application>

</manifest>